{
  "expo": {
    "name": "Portal",
    "slug": "Portal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoEstatIcon.png",  
    "splash": {
      "image": "./assets/LogoEnce.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9D9AB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/EnceLogo.png"
    },
    "packagerOpts": {
      "sourceExts": [ "js", "json", "ts", "tsx", "jsx", "vue"]
    }
  }
}
